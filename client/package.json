{
  "name": "vue-cli3",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@jd/wyg-jrtc-test": "0.0.1",
    "@vue/eslint-config-prettier": "^6.0.0",
    "axios": "^0.18.0",
    "core-js": "^3.6.4",
    "draggable": "^4.2.0",
    "element-ui": "^2.4.5",
    "eslint-plugin-prettier": "^3.1.2",
    "file-saver": "^2.0.2",
    "js-md5": "^0.7.3",
    "node-gyp": "^3.8.0",
    "node-sass": "^4.14.1",
    "rebuild": "^0.1.2",
    "sortablejs": "^1.8.3",
    "vue": "^2.5.17",
    "vue-router": "^3.0.1",
    "vuedraggable": "^2.17.0",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.2.0",
    "@vue/cli-plugin-eslint": "^3.2.0",
    "@vue/cli-service": "^3.2.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-component": "^1.1.1",
    "eslint": "^5.10.0",
    "eslint-plugin-vue": "^5.0.0-0",
    "sass-loader": "^7.0.1",
    "vue-cli-plugin-element": "^1.0.0",
    "vue-template-compiler": "^2.5.17"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "rules": {
      "strict": 2,
      "semi": [
        2,
        "always"
      ],
      "no-plusplus": 0,
      "radix": 2,
      "eqeqeq": 2,
      "no-var": 0
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
